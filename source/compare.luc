module compare (  
    input b[16],
    input a[16],
    input alufn[6],  
    output out[16]
  ) {
   sig cmp;  // Signal for storing the least significant bit(LSB) of the output
  always {
  cmp=0;
    out=16b0;
    case(alufn[2:0]){
      b01:
        cmp=($signed(a)==$signed(b));
      b10:
        cmp=($signed(a)<$signed(b));
      b11:
        cmp=($signed(a)<=$signed(b));
      }
    out[15:1]=15b0;
    out[0]=cmp;
  }
}
